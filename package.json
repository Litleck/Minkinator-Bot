{
  "name": "minkinator",
  "author": "Litleck",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Litleck/Minkinator.git"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "canvas": "*",
    "chalk": "^4.1.0",
    "discord.js": "discordjs/discord.js",
    "entities": "^2.0.3",
    "google-tts-api": "^2.0.2",
    "moment": "^2.27.0",
    "node-fetch": "^3.1.1",
    "pluralize": "^8.0.0",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^7.0.0",
    "python-shell": "^2.0.3",
    "sequelize": "^6.5.1",
    "sqlite3": "^5.0.0",
    "twemoji": "^13.0.1",
    "ytdl-core-discord": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0"
  },
  "main": "./src/index.js",
  "scripts": {
    "start": "node .",
    "packages": "npm i",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 11
    },
    "globals": {
      "__basedir": "readonly",
      "util": "readonly",
      "Discord": "readonly"
    },
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-trailing-spaces": "error"
    }
  }
}
